	<queryString language="SQL">
		<![CDATA[select  l.Nombre As Cliente,l.Apellido,p.Numer_Facturas,h.Nombre As Producto, Fecha
from VentFacturas p
inner join Cliente l on l.ID_Cliente=p.ID_Cliente
inner join Productos h on h.ID_Producto=h.ID_Producto
order by h.ID_Producto desc
]]>
	</queryString>
