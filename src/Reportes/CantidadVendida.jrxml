	<queryString language="SQL">
		<![CDATA[SELECT 
    p.ID_Producto,
    p.Nombre As Producto,
    k.Cantidad,
	l.Fecha
FROM 
    Productos p
	inner join DetalleVentaFactura k on k.ID_Producto=p.ID_Producto
	inner join VentFacturas l on l.Numer_Facturas=k.Numer_Factura
ORDER BY 
    p.Nombre]]>
	</queryString>
